from _typeshed import Incomplete
from talon.api import ffi as ffi, lib as lib
from typing import Callable

AudioQueueCallback = Callable[[int, list[float]], int]

class AudioQueue:
    handle: ffi.CData
    ffi_self: ffi.CData
    cb: Incomplete
    def __init__(self, min_latency: int, max_latency: int, cb: AudioQueueCallback, async_queue: bool = ...) -> None: ...
    def close(self) -> None: ...
    def sink(self, samples: list[float]) -> None: ...
    def __del__(self) -> None: ...
