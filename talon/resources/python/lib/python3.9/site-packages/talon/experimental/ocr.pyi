from talon import debug as debug
from talon.api import ffi as ffi, ffi_string as ffi_string, lib as lib
from talon.skia import Image as Image
from talon.types import Rect as Rect
from typing import Union

class Bounds:
    rects: list[Rect]
    def __getitem__(self, key: Union[int, slice]) -> Rect: ...
    def __init__(self, rects) -> None: ...

class Result:
    text: str
    confidence: float
    rect: Rect
    bounds: Bounds
    def __init__(self, text, confidence, rect, bounds) -> None: ...

def ocr(image: Image) -> list[Result]: ...
